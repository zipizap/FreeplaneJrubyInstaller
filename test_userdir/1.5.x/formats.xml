<?xml version="1.0" encoding="UTF-8"?>
<!-- 'type' selects the kind of data the formatter is intended to format. -->
<!-- 'style' selects the formatter implementation: -->
<!--   - 'date': http://download.oracle.com/javase/6/docs/api/java/text/SimpleDateFormat.html -->
<!--   - 'decimal': http://download.oracle.com/javase/6/docs/api/java/text/DecimalFormat.html -->
<!--   - 'formatter': http://download.oracle.com/javase/6/docs/api/java/util/Formatter.html -->
<!--   - 'name': a informal name, a comment that's not visible in the app -->
<!--   - 'locale': the name of the locale, only set for locale dependent format codes -->
<formats>
    <format type="number" style="decimal" name="default number" locale="en_US">#0.####</format>
    <format type="number" style="decimal" name="decimal" locale="en_US">#.00</format>
    <format type="number" style="decimal" name="integer" locale="en_US">#</format>
    <format type="number" style="decimal" name="percent" locale="en_US">#.##%</format>
    <format type="date" style="date" name="short date" locale="en_US">M/d/yy</format>
    <format type="date" style="date" name="medium date" locale="en_US">MMM d, yyyy</format>
    <format type="date" style="date" name="short datetime" locale="en_US">M/d/yy h:mm a</format>
    <format type="date" style="date" name="medium datetime" locale="en_US">MMM d, yyyy h:mm a</format>
    <format type="date" style="date" name="short iso date" locale="en_US">yyyy-MM-dd</format>
    <format type="date" style="date" name="long iso date" locale="en_US">yyyy-MM-dd HH:mm</format>
    <format type="date" style="date" name="full iso date" locale="en_US">yyyy-MM-dd&apos;T&apos;HH:mmZ</format>
    <format type="date" style="date" name="time" locale="en_US">HH:mm</format>
</formats>
